    "ember-test-selectors": "^0.3.2",
    "ember-try": "^1.0.0",
    "eslint": "^7.1.0",
    "eslint-plugin-ember": "^8.6.0",
    "eslint-plugin-node": "^11.1.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5"
